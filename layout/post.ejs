
<article id="post" class="post">
  <div id="post-head" class="post-head" style="background-image: url(<%=page.thumbnail%>);">
    <h1><%=page.title%></h1>
  </div>
  
  <div id="post-content" class="post-content">
    <%-page.content%>
  </div>
  
</article>

<div id="paginator">
  <%-paginator()%>
</div>
